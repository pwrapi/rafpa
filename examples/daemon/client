#!/bin/sh
POWERAPI=/opt/PowerAPI
POWERAPI_BIN=$POWERAPI/bin
POWERAPI_LIB=$POWERAPI/lib


export PATH="$POWERAPI_BIN:${PATH}"

export LD_LIBRARY_PATH="$POWERAPI_LIB:${LD_LIBRARY_PATH}"
export DYLD_LIBRARY_PATH="$POWERAPI_LIB:${DYLD_LIBRARY_PATH}"
export POWERAPI_CONFIG="/root/PowerAPI_Working_Code/PowerAPI-Redfish/config/daemon_redfish.xml"
export POWERAPI_SERVER_PORT="15000"
export POWERAPI_SERVER="localhost"
export POWERAPI_DEBUG=0
export POWERAPI_ROOT=plat
